## Proc
genfscon proc /simslot_count                                                                                   u:object_r:proc_simslot_count:s0
genfscon proc /sys/vm/swappiness                                                                               u:object_r:proc_swappiness:s0

## Sysfs
# Class
genfscon sysfs /class/video4linux                                                                              u:object_r:sysfs_v4l:s0

# Devices
genfscon sysfs /devices/platform/13200000.usb/13200000.dwc3/udc/13200000.dwc3/current_speed                      u:object_r:sysfs_usb:s0
genfscon sysfs /devices/platform/13860000.hsi2c/i2c-1/1-0049/sm5714-charger/power_supply/sm5714-charger/type     u:object_r:sysfs_battery:s0
genfscon sysfs /devices/platform/13860000.hsi2c/i2c-1/1-0049/sm5714-fuelgauge/power_supply/sm5714-fuelgauge/type u:object_r:sysfs_battery:s0
genfscon sysfs /devices/platform/13860000.hsi2c/i2c-1/1-0049/sm5714-charger/power_supply/sm5714-otg/type         u:object_r:sysfs_battery:s0
genfscon sysfs /devices/platform/acpm_mfd_bus@11a00000/i2c-11/11-001f/s2mpu13-gpadc/iio:device0/name             u:object_r:sysfs_iio:s0
genfscon sysfs /devices/platform/mfc/video4linux/video6/name                                                     u:object_r:sysfs_v4l:s0
genfscon sysfs /devices/platform/mfc/video4linux/video7/name                                                     u:object_r:sysfs_v4l:s0
genfscon sysfs /devices/system/chip-id/revision                                                                  u:object_r:sysfs_chipid:s0

genfscon sysfs /devices/virtual/sensors/light_sensor/hall_ic                                                     u:object_r:sysfs_sensors_writable:s0

# Firmware
genfscon sysfs /firmware/devicetree/base/sound/mixer-paths                                                     u:object_r:sysfs_audio:s0

# Kernel
genfscon sysfs /kernel/mm/vmscan/mem_boost_mode                                                                u:object_r:sysfs_camera_writable:s0

# Module
genfscon sysfs /module/scsc_bt/parameters/bluetooth_address                                                    u:object_r:sysfs_bt_writable:s0
